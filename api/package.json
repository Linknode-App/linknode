{
  "name": "imgelo-api",
  "version": "1.0.0",
  "scripts": {
    "dev": "dotenv -- tsx watch src/server.ts",
    "dev:sync": "dotenv -e .env.debug -- tsx watch src/syncer.ts",
    "build": "pkgroll",
    "start": "node ./dist/server.cjs",
    "db:schema": "node ./meta/scripts/dumpdb.js",
    "gen:typeorm": "oort -d imgelo -s imgelo,imgelo_private -o ./src/generated/entities",
    "gen:entities": "tsx ./meta/scripts/concatEntities.ts",
    "env:sync": "node ./meta/scripts/syncEnv.js",
    "typeorm:format": "biome format --write ./src/generated/entities/*.ts"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/server.mjs",
  "types": "./dist/server.d.ts",
  "dependencies": {
    "@ai-sdk/anthropic": "^1.2.9",
    "@ai-sdk/deepseek": "^0.2.8",
    "@ai-sdk/google": "^1.2.10",
    "@ai-sdk/openai": "^1.3.9",
    "@ai-sdk/openai-compatible": "^0.2.6",
    "@ai-sdk/togetherai": "^0.2.8",
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/s3-request-presigner": "^3.787.0",
    "@biomejs/js-api": "^0.7.1",
    "ai": "^4.3.4",
    "apple-signin-auth": "^1.7.8",
    "astrooort": "^0.0.2",
    "await-exec": "^0.1.2",
    "bcrypt": "^5.1.1",
    "cloudflare": "^4.2.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "glob": "^11.0.1",
    "google-auth-library": "^9.15.1",
    "graphile-worker": "^0.16.6",
    "jsonwebtoken": "^9.0.2",
    "jwt-simple": "^0.5.6",
    "moment": "^2.30.1",
    "node-html-parser": "^7.0.1",
    "nomadic": "^0.2.7",
    "openai": "^4.96.2",
    "pg": "^8.13.2",
    "pg-listen": "^1.7.0",
    "pkgroll": "^2.8.0",
    "puppeteer": "^24.7.2",
    "reflect-metadata": "^0.2.2",
    "serpapi": "^2.1.0",
    "socket.io": "^4.8.1",
    "ts-morph": "^25.0.1",
    "ts-to-zod": "^3.15.0",
    "typeid-js": "^1.2.0",
    "typeorm": "^0.3.20",
    "uuidv7": "^1.0.2",
    "zod": "^3.24.2",
    "zod-to-ts": "^1.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@biomejs/wasm-nodejs": "^1.9.4",
    "@flydotio/dockerfile": "^0.7.10",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.24",
    "@types/pg": "^8.11.11",
    "copy": "^0.3.2",
    "dotenv-cli": "^8.0.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.24.0"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/server.d.cts",
        "default": "./dist/server.cjs"
      },
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      }
    }
  }
}
